/* 
1. shift
2. control
3. backspace
4. enter
5. tab
6. shift enter
7. slash
8. esc
9. sleep

== Base ==
= Left =
qw - esc
wf
rs - lshift
xc - tab
fp - line feed
st - lcontrol
cv
wfp
rst
xcv

= Right =
y' - sleep
uy
ei - rshift
,. - slash
lu - enter
ne - rcontrol
m, - backspace
luy
nei
m,.

= Both =
tn - capsword
*/

COMB(esc_combo, KC_ESC, KC_Q, KC_W)
COMB(slep_combo, KC_SLEP, KC_Y, KC_QUOT) 
COMB(bspc_combo, KC_BSPC, KC_M, KC_COMM)

COMB(lshft_combo, KC_LSFT, KC_R, KC_S)
COMB(lctrl_combo, KC_LCTL, KC_S, KC_T)
COMB(rshft_combo, KC_RSFT, KC_E, KC_I)
COMB(rctrl_combo, KC_RCTL, KC_N, KC_E)

COMB(tab_combo, KC_TAB, KC_X, KC_C)
COMB(ent_combo, KC_ENT, KC_L, KC_U)
COMB(lf_combo, LSFT(KC_ENTER), KC_F, KC_P)

COMB(slsh_combo, KC_SLSH, KC_DOT, KC_COMM)

COMB(capsw_combo, CW_TOGG, KC_T, KC_N)
